<UserControl x:Class="vpam.objektas"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:vpam"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>

        <StackPanel Margin="10" Height="90" Width="110">
            <Rectangle x:Name="objektasUI" 
         Height="50" Width="50"
         Fill="red" />
            <ComboBox Name="Pasirinkimas" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Height="30" SelectedValuePath="Content">
                <ComboBoxItem Content="Input"></ComboBoxItem>
                <ComboBoxItem Content="Output"></ComboBoxItem>
            </ComboBox>
        </StackPanel>
    </Grid>
</UserControl>
