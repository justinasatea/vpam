<UserControl x:Class="VPAM.Zymeklis"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VPAM"
             mc:Ignorable="d" 
            d:DesignHeight="100" d:DesignWidth="100"
             >
    <Grid ToolTip="Čia paleisti naują elementą">
        <Ellipse x:Name="elipse" Height="85" Width="85" Fill="#FF73503C" Stroke="Black" Margin="5,6,10,9"/>
        <Ellipse Fill="Black"  Height="75" Stroke="Black" Margin="10,10,15,15"/>
        <Rectangle x:Name="x1" Height="25" Width="7" Fill="Yellow" Margin="45,57,48,18" RadiusY="4.5" RadiusX="4.5" Stroke="Black"/>
        <Rectangle x:Name="x2" Width="25" Height="5" Fill="Yellow" Margin="36,0,39,28" RadiusY="4.5" RadiusX="4.5" VerticalAlignment="Bottom"/>
        <Label x:Name="tekstas" FontSize="10" Content="   Pridėti &#xA;galima čia" Margin="17,19,20,43" Foreground="#FF18F50D"/>
    </Grid>
</UserControl>
